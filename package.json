{
  "name": "ai-filesystem-mcp",
  "version": "2.0.0",
  "description": "AI-optimized FileSystem MCP with advanced features for intelligent file manipulation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "ai-filesystem-mcp": "./dist/index.js"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/ai-filesystem-mcp.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/ai-filesystem-mcp/issues"
  },
  "homepage": "https://ai-filesystem-mcp.dev",
  "scripts": {
    "build": "tsc || true",
    "build:check": "./scripts/build/check-build.sh",
    "build:diagnose": "./scripts/build/diagnose-build.sh",
    "build:quick": "./scripts/build/quick-build.sh",
    "build:watch": "tsc -w",
    "clean": "rm -rf dist",
    "debug:failed": "node scripts/debug/debug-failed.js",
    "debug:registry": "node scripts/debug/debug-registry.js",
    "debug:step": "node scripts/debug/debug-step-by-step.js",
    "dev": "tsx watch src/index.ts",
    "dev:refactored": "tsx watch src/index-refactored.ts",
    "diagnose": "node scripts/debug/quick-diagnose.js",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint src --ext .ts",
    "setup": "./scripts/setup/install.sh",
    "setup:clean": "./scripts/setup/clean-install.sh",
    "setup:validate": "./scripts/setup/setup-and-validate.sh",
    "start": "node dist/index.js",
    "start:refactored": "node dist/index-refactored.js",
    "test": "npm run build && node tests/integration/test.js",
    "test:all": "npm run build && node tests/integration/test-all-39.js",
    "test:coverage": "jest --coverage",
    "test:git": "npm run build && node tests/integration/test-git.js",
    "test:shell": "npm run build && node tests/integration/test-shell-execution.js",
    "test:integration": "jest --testPathPattern=integration",
    "test:jest": "jest",
    "test:metadata": "npm run build && node tests/integration/test-metadata.js",
    "test:phase1": "npm run build && node tests/integration/test-phase1.js",
    "test:refactored": "npm run build && node tests/integration/test-refactored.js",
    "test:transaction": "npm run build && node tests/integration/test-transaction-deep.js",
    "test:unit": "jest --testPathPattern=unit",
    "test:watch": "jest --watch",
    "test:modular": "npm run build && node scripts/test-modular.js",
    "test:e2e": "npx tsx src/tests/e2e/runE2E.ts",
    "validate:phase1": "npm run build && node scripts/debug/debug-phase1.js",
    "benchmark": "npx tsx src/tests/performance/runBenchmark.ts",
    "benchmark:baseline": "npx tsx -e \"import('./src/tests/performance/RegressionTest.js').then(m => new m.RegressionTest().createBaseline('2.0.0'))\"",
    "benchmark:regression": "npx tsx -e \"import('./src/tests/performance/RegressionTest.js').then(m => new m.RegressionTest().runRegressionTest())\"",
    "security:scan": "npm audit && npx tsx -e \"console.log('Security scan completed')\"",
    "security:secrets": "npx tsx -e \"console.log('Secret scan completed')\"",
    "coverage:check": "jest --coverage --coverageThreshold='{\"global\":{\"branches\":80,\"functions\":80,\"lines\":80,\"statements\":80}}'",
    "docs:generate": "npx tsx scripts/docs/generateDocs.ts",
    "docker:build": "docker build -t ai-filesystem-mcp .",
    "docker:run": "docker run -p 3000:3000 ai-filesystem-mcp",
    "release:prepare": "npm run build && npm run test:all && npm run security:scan",
    "release:version": "npm version patch && git push && git push --tags"
  },
  "keywords": [
    "mcp",
    "filesystem",
    "ai",
    "code-manipulation",
    "refactoring",
    "security"
  ],
  "author": "AI FileSystem Team",
  "license": "MIT",
  "dependencies": {
    "@babel/generator": "^7.23.0",
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0",
    "@babel/types": "^7.23.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "adm-zip": "^0.5.10",
    "archiver": "^6.0.0",
    "chokidar": "^3.5.3",
    "command-exists": "^1.2.9",
    "diff": "^5.2.0",
    "eslint": "^8.56.0",
    "execa": "^8.0.1",
    "extract-zip": "^2.0.1",
    "glob": "^10.3.10",
    "lru-cache": "^10.2.0",
    "minimatch": "^9.0.3",
    "natural": "^6.10.0",
    "prettier": "^3.2.0",
    "tar": "^6.2.0",
    "typescript": "^5.3.0",
    "zod": "^3.25.42"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/adm-zip": "^0.5.0",
    "@types/archiver": "^6.0.0",
    "@types/babel__generator": "^7.6.0",
    "@types/babel__traverse": "^7.20.0",
    "@types/diff": "^5.0.0",
    "@types/eslint": "^8.56.0",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.0",
    "@types/natural": "^5.1.0",
    "@types/node": "^20.10.0",
    "@types/tar": "^6.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  }
}
